export API_NAME="DevAPI"
export APIEndpoint=https://3mse4mrdbnh6xcdhjxhyswokla.appsync-api.eu-west-1.amazonaws.com/graphql
export USER_POOL_ID=eu-west-1_AK2EGwece
export CLIENT_ID=1gljr1mskmvu51js5lq0n7l4ks

CREDS=$(aws sts assume-role --role-arn arn:aws:iam::562925407179:role/bttv-mnb-poc-role-admin --role-session-name some-session --duration-seconds 3600 --profile admin)

AWS_ACCESS_KEY_ID=$(echo ${CREDS} | jq --raw-output ".Credentials[\"AccessKeyId\"]")
AWS_SECRET_ACCESS_KEY=$(echo ${CREDS} | jq --raw-output ".Credentials[\"SecretAccessKey\"]")
AWS_SESSION_TOKEN=$(echo ${CREDS} | jq --raw-output ".Credentials[\"SessionToken\"]")

export AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID}
export AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY}
export AWS_SESSION_TOKEN=${AWS_SESSION_TOKEN}
